package com.main;

import java.util.Scanner;

public class NameAndAgeOfPlayerMain {

	public static void main(String[] args) {
		String name;
		int age;
		Scanner scanner=new Scanner(System.in);
		System.out.println("Enter name :");
		name=scanner.next();
		System.out.println("Enter age :");
		age=scanner.nextInt();
		
		
	}

}
